{
  "name": "ui-harness",
  "version": "3.7.0",
  "description": "Isolate, test and document modular UI with React.",
  "main": "./lib/server",
  "scripts": {
    "build:watch": "npm run build -- --watch",
    "build": "rm -rf lib && ./node_modules/babel-cli/bin/babel.js src --out-dir lib --source-maps",
    "dev": "QUICK_BUILD=true ./node_modules/.bin/nodemon -x \"./node_modules/babel-cli/bin/babel-node.js start.js\"",
    "lint": "eslint --ext .js,.jsx ./src",
    "prepublish": "npm test && npm run lint && npm run build",
    "start": "node start",
    "tdd": "npm run test -- --reporter min --watch",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "babel-relay-plugin": "^0.6.3",
    "bluebird": "^3.2.1",
    "chalk": "^1.1.1",
    "color": "^0.11.1",
    "compression": "^1.5.2",
    "express": "^4.13.0",
    "express-graphql": "^0.4.5",
    "file-loader": "^0.8.4",
    "filesize": "^3.1.5",
    "fs-extra": "^0.26.4",
    "graphql": "^0.4.14",
    "graphql-relay": "^0.3.6",
    "immutable": "^3.7.4",
    "invariant": "^2.2.0",
    "js-babel": "^6.0.0",
    "js-babel-ui": "^6.0.0",
    "js-bdd": "^2.0.0",
    "js-util": "^2.0.2",
    "js-yaml": "^3.5.2",
    "json-loader": "^0.5.2",
    "marked": "^0.3.5",
    "minimist": "^1.2.0",
    "moment": "^2.11.1",
    "node-libs-browser": "^1.0.0",
    "radium": "^0.16.4",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-atoms": "^2.0.9",
    "react-dom": "^0.14.7",
    "react-object": "^2.0.11",
    "react-relay": "^0.6.1",
    "react-schema": "^1.1.0",
    "semver": "^5.1.0",
    "shelljs": "^0.6.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "chai": "^3.5.0",
    "js-babel-dev": "^6.0.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "sinon": "^1.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/ui-harness.git"
  },
  "keywords": [
    "ui",
    "testing",
    "react",
    "modular",
    "design",
    "react-component"
  ],
  "author": "Phil Cockfield",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philcockfield/ui-harness/issues"
  },
  "homepage": "http://uiharness.com"
}
